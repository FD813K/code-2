import java.util.Scanner;

public class Mavenproject2 {

    static void checkpass(String password)
    {
        String specialChars ="!@#$%^&*()_+{}|[]\\;',./:<>?~";
        boolean hasSpecial = false;
        boolean hasNumber = false;
        boolean hasLetter = false;

        for(int i=0; i<password.length(); i++)
        {
            char ch = password.charAt(i);

            if(specialChars.contains(Character.toString(ch)))
            {
                hasSpecial = true;
            }
            if(Character.isDigit(ch))
            {
                hasNumber = true;
            }
            if(Character.isLetter(ch))
            {
                hasLetter = true;
            }
        }

        if(password.length() < 6)
        {
            System.out.println("Password is Too Short");
        }
        else if(password.length() <= 10)
        {
            if((hasLetter && !hasNumber && !hasSpecial) || (!hasLetter && hasNumber && !hasSpecial))
            {
                System.out.println("Password is Weak");
            }
            else if(hasLetter && hasNumber)
            {
                System.out.println("Password is Medium");
            }
            else
            {
                System.out.println("Password is Weak");
            }
        }
        else 
        {
            if(hasLetter && hasNumber && hasSpecial)
            {
                System.out.println("Password is Strong");
            }
            else
            {
                System.out.println("Password is Medium");
            }
        }
    }

    public static void main(String[] args)
    {
        Scanner x=new Scanner(System.in);

        System.out.println("Enter the password:");
        String password = x.nextLine();

        checkpass(password);
    }
}
